@model modelbindtab.Models.LibraryModel

<h1>Create Library</h1>
<form asp-action = "Create" method = "post">
    <div>
        <label asp-for = "Name"></label>
        <input type = "text" asp-for = "Name" />
        <span asp-validation-for="Name"></span>
    </div>
    <div>
        <label asp-for = "Stall"></label>
        <input type="text" asp-for = "Stall" />
    </div>
    <div>
        <label asp-for = "Address"></label>
        <input type = "text" asp-for = "Address" />
    </div>
    <div>
        <label asp-for = "OpenDate"></label>
        <input type = "date" asp-for = "OpenDate" />
    </div>
    <div>
        <label asp-for = "Email"></label>
        <input type = "email" asp-for = "Email" />
    </div>
    <button type = "submit">Create</button>
</form>

